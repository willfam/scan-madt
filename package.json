{
    "name": "lta-madt-gui",
    "version": "3.2.9",
    "private": true,
    "scripts": {
        "build": "ng build",
        "start": "ng serve",
        "start:local:network": "ng serve --host=0.0.0.0 --disable-host-check",
        "test": "ng test",
        "ng:clear:cache": "ng cache clean",
        "test:clean": "ng cache clean",
        "lint:check": "npm run lint:check:eslint && npm run lint:check:prettier",
        "lint:check:eslint": "npx eslint .",
        "lint:check:prettier": "npx prettier --check './**/*.{ts,tsx,js,jsx,md,json,css,scss,less}'",
        "lint:fix": "npm run lint:fix:eslint && npm run lint:fix:prettier",
        "lint:fix:eslint": "npx eslint --fix .",
        "lint:fix:prettier": "npx prettier --write './**/*.{ts,tsx,js,jsx,md,json,css,scss,less}'",
        "prepare": "husky",
        "start:build": "lite-server",
        "serve:ssr:lta-btds-gui": "node dist/lta-btds-gui/server/server.mjs",
        "watch": "ng build --watch --configuration development"
    },
    "dependencies": {
        "@angular/animations": "^17.0.0",
        "@angular/cdk": "^17.3.2",
        "@angular/common": "^17.0.0",
        "@angular/compiler": "^17.0.0",
        "@angular/core": "^17.0.0",
        "@angular/forms": "^17.0.0",
        "@angular/material": "^17.3.2",
        "@angular/platform-browser": "^17.0.0",
        "@angular/platform-browser-dynamic": "^17.0.0",
        "@angular/platform-server": "^17.0.0",
        "@angular/router": "^17.0.0",
        "@angular/ssr": "^17.0.10",
        "@ngrx/signals": "^17.2.0",
        "@ngrx/store": "^17.2.0",
        "@ngx-translate/core": "^15.0.0",
        "@ngx-translate/http-loader": "^8.0.0",
        "@types/mqtt": "^2.5.0",
        "express": "^4.18.2",
        "mqtt": "^5.9.1",
        "ngx-mqtt": "^17.0.0",
        "ngx-scrollbar": "^15.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.10",
        "@angular-eslint/builder": "17.3.0",
        "@angular-eslint/eslint-plugin": "17.3.0",
        "@angular-eslint/eslint-plugin-template": "17.3.0",
        "@angular-eslint/schematics": "17.3.0",
        "@angular-eslint/template-parser": "17.3.0",
        "@angular/cli": "^17.0.10",
        "@angular/compiler-cli": "^17.0.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/node": "^18.18.0",
        "@typescript-eslint/eslint-plugin": "7.2.0",
        "@typescript-eslint/parser": "7.2.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-simple-import-sort": "12.1.0",
        "husky": "9.0.11",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "15.2.2",
        "lite-server": "^2.6.1",
        "prettier": "3.2.5",
        "prettier-eslint": "^16.3.0",
        "typescript": "~5.2.2",
        "typescript-eslint": "7.8.0"
    }
